{
    "name": "cslant/laravel-generator",
    "description": "Laravel Generator",
    "homepage": "https://github.com/cslant/laravel-generator",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "generator",
        "crud generator",
        "migration generator",
        "model generator",
        "controller generator",
        "repository",
        "request",
        "resource",
        "factory",
        "seeder",
        "action",
        "laravel generator",
        "cslant"
    ],
    "autoload": {
        "psr-4": {
            "CSlant\\LaravelGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CSlant\\LaravelGenerator\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tan Nguyen",
            "email": "tannp27@gmail.com",
            "homepage": "https://tanhongit.com",
            "role": "Lead"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "laravel/framework": "^9.0|^10.0",
        "cslant/laravel-generator-ui": "^v1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^v3.0",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.3",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "support": {
        "issues": "https://github.com/cslant/laravel-generator/issues"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CSlant\\LaravelGenerator\\Providers\\LaravelGeneratorServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    }
}
